#helps
	padding 90px 0 0
	.container
		position relative
.general-img-helps-1
.general-img-helps-2
	top 50px
	z-index 20
	left -100px
.general-img-helps-2
	left auto
	right -100px
.helps-item
	text-align center
	position relative
	z-index 40
	margin-bottom: 70px
	&-img
		display block
		margin 0 auto 50px
		width 100px
		height 100px
		background-image url(../img/icons/helps.png)
		background-repeat no-repeat
		&-1
			background-position: 0 0
		&-2
			background-position: -100px 0
		&-3
			background-position: -200px 0
		&-4
			background-position: -300px 0
		&-5
			background-position: -400px 0
	&-text
		font-weight: 600